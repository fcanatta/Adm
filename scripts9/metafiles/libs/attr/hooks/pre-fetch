#!/usr/bin/env bash
set -Eeuo pipefail
: "${CATEGORY:=libs}"; : "${NAME:=attr}"
: "${ADM_ROOT:=/usr/src/adm}"

TRACKER="${TRACKER:-${ADM_ROOT}/scripts/10.10-update-upstream-tracker.sh}"

[[ -x "$TRACKER" ]] && "$TRACKER" \
  --category "$CATEGORY" \
  --name "$NAME" \
  --download \
  --apply \
  --provider auto \
  --parallel "${JOBS:-4}" || true
