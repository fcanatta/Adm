#!/usr/bin/env bash
set -Eeuo pipefail
: "${CATEGORY:=sys}"; : "${NAME:=util-linux}"
: "${ADM_ROOT:=/usr/src/adm}"; TRACKER="${ADM_ROOT}/scripts/10.10-update-upstream-tracker.sh"
[[ -x "$TRACKER" ]] && "$TRACKER" --category "$CATEGORY" --name "$NAME" --download --apply --provider auto --parallel "${JOBS:-4}" || true
